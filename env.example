# Career RAG Coach Environment Variables
# Copy this file to .env and fill in your actual values

# Required: OpenAI API Key for LLM functionality
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Model Selection
OPENAI_MODEL=gpt-4o-mini

# Optional: Logging Level
LOG_LEVEL=INFO

# Optional: LinkedIn Scraping Settings
LINKEDIN_DELAY=2
LINKEDIN_MAX_RETRIES=3

# Optional: YouTube Search Settings
YOUTUBE_MAX_RESULTS=3
YOUTUBE_MAX_SKILLS=8
